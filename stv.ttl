: a skos:ConceptScheme;
        dct:title "OER Service type vocabulary"@en ;
        dct:description "This vocabulary was created and curated within the OER Worldmap project to classify services within the Open Educational Resources (OER) space. Under the term 'OER service' fall online offers by organizations or individuals that somehow help people in finding, assessing, creating or publishing OER, e.g. repositories, referatories, registries etc."@en ;
        dct:publisher <http://lobid.org/organisation/DE-605> ;
        vann:preferredNamespaceUri "https://w3id.org/class/stv/" ;
        vann:preferredNamespacePrefix "stv" ;
        skos:hasTopConcept :repository, :textbooks, :moocs, :referatory .

:collections a skos:Collection ;
           skos:prefLabel "Service types that are collections of Open Educational Resources."@en ;
           skos:member :repository ;
           skos:member :textbooks ;
           skos:member :moocs .

:repository a skos:Concept ;
           skos:topConceptOf : ;
           skos:inScheme : ;
           skos:prefLabel "OER Repository"@en .

:textbooks a skos:Concept ;
           skos:topConceptOf : ;
           skos:inScheme : ;
           skos:prefLabel "Textbook collection"@en .

:moocs a skos:Concept ;
           skos:topConceptOf : ;
           skos:inScheme : ;
           skos:prefLabel "MOOC platform"@en .

:referatory a skos:Concept ;
           skos:topConceptOf : ;
           skos:inScheme : ;
           skos:prefLabel "Referatory"@en .
